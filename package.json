{
  "name": "elasticsearch-index-migrate",
  "description": "elasticsearch-index-migrate",
  "version": "0.0.0",
  "author": "kmiura @kmiura-1002",
  "bin": {
    "elasticsearch-index-migrate": "./bin/run"
  },
  "bugs": "https://github.com/kmiura-1002/elasticsearch-index-migrate/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/config": "^0.0.36",
    "config": "^3.2.5",
    "es6": "npm:@elastic/elasticsearch@^6.7.0",
    "es7": "npm:@elastic/elasticsearch@^7.0.0",
    "prettier": "^1.19.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kmiura-1002/elasticsearch-index-migrate",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "elasticsearch-index-migrate",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kmiura-1002/elasticsearch-index-migrate",
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
